    <div class="container">
        
        <div class="row karl-new-arrivals">
            {{#each Product}}
            <!-- Single gallery Item Start -->
            <div class="col-12 col-sm-6 col-md-4 single_gallery_item women wow fadeInUpBig" data-wow-delay="0.2s">
                <!-- Product Image -->
                <div class="product-img">

                   
                    <img src="/uploads/{{images}}" alt="Image" width="250px" height="100px">
                     

                    <div class="product-quicview">
                        <a href="/quickview" data-toggle="modal" data-target="#quickview"><i class="ti-plus"></i></a>
                    </div>
                </div>
                <!-- Product Description -->
                <div class="product-description">
                    <h3 class="product-price">{{this.brand}}</h3>
                    <h6>{{this.productname}}</h6>
                  
                    <h6>Price:   {{this.price}}</h6>
                      <p>{{this.description}}</p>
                    <!-- Add to Cart -->
                    <a href="#" class="add-to-cart-btn">ADD TO CART</a>
                </div>
            </div>
            {{/each}}
        </div>
       
    </div>
</section>























